#set ($app_port = $numberUtil.createInteger(${doucome_appserver_port}) )
<IfModule mod_proxy.c>
	ProxyRequests Off
	ProxyPreserveHost On	
	
	ProxyPassMatch ^(/(s|member|admin|product|order|common|home|search|validator)/(.*))$ http://localhost:${app_port} min=5 smax=16 max=64 ttl=600 timeout=30
	
</IfModule>
